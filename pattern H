for i in range(1,10):
    for j in range(1,7):
        if i==1 or i==2 or i==4 or i==3:
            if j==1 or j==6:
                print('*',end=' ')
            else:
                print(' ',end=' ')
        elif i==5:
            print('*',end=' ')
        else:
            if j==1 or j==6:
                 print('*',end=' ')
            else:
                print(' ',end=' ')
    print()
